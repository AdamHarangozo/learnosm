msgid "a new password"
msgstr ""

# 3178ff22fbdb494991ab92bb1c5ae639
#: ../../beginner/moving_forward.rst:63
msgid "repeat the password"
msgstr ""

# 7c654f3625ef4ad29e96edfb10b08b62
